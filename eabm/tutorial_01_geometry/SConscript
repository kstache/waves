#! /usr/bin/env python

# Inherit the parent construction environment
Import('env')

journal_file = "single_element_geometry"
journal_options = ""

tutorial_01_geometry = env.Command(
    target=[f"{journal_file}.cae", f"{journal_file}.jnl"],
    source=[f"{env['PROJECT_DIR']}/eabm/abaqus/{journal_file}.py"],
    action=f"abaqus cae -noGui ${{SOURCE.abspath}} -- {journal_options} > ${{SOURCE.filebase}}.log 2>&1")
